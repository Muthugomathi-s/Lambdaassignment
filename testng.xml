<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="[Demo] Cross Browser Testing Using Selenium" parallel="methods" thread-count="5">
<test name="[Demo] Cross Browser Testing Using Selenium"/>
   <test name="[LambdaTest] Test Scenario - 1">
       <parameter name="browser" value="Chrome"/>
       <parameter name="version" value="88.0"/>
       <parameter name="platform" value="Windows 10"/>
       <parameter name="resolution" value="1024x768"/>
    <classes>
      <class name="lambda.Lambdaproject.Scenario1"/>
     </classes>
</test>

   <test name="[LambdaTest] Test Scenario - 2">
       <parameter name="browser" value="Firefox"/>
       <parameter name="version" value="87.0"/>
       <parameter name="platform" value="Windows 7"/>
       <parameter name="resolution" value="1024x768"/>
  <classes>
      <class name="lambda.Lambdaproject.Scenario1"/>
 </classes>
  </test> <!-- Test -->

   <test name="[LambdaTest] Test Scenario - 3">
       <parameter name="browser" value="MicrosoftEdge"/>
       <parameter name="version" value="87.0"/>
       <parameter name="platform" value="macOS Sierra"/>
       <parameter name="resolution" value="1024x768"/>
<classes>
      <class name="lambda.Lambdaproject.Scenario1"/>
 </classes>
   </test>

    <test name="[LambdaTest] Test Scenario - 4">
       <parameter name="browser" value="Internet Explorer"/>
       <parameter name="version" value="11.0"/>
       <parameter name="platform" value="Windows 10"/>
       <parameter name="resolution" value="1024x768"/>
<classes>
      <class name="lambda.Lambdaproject.Scenario1"/>
 </classes>
   </test>
</suite> <!-- Suite -->
